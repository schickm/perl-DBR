CREATE TABLE TBL(foo) (
    id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    data VARCHAR(255) NOT NULL
);

CREATE TABLE TBL(bar) (
  id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
  foo_id INTEGER NOT NULL,
  data VARCHAR(255) NOT NULL
);

INSERT INTO TBL(foo) VALUES(1,'One');
INSERT INTO TBL(foo) VALUES(2,'Two');
INSERT INTO TBL(foo) VALUES(3,'Three');

-- intentionally no data in bar
