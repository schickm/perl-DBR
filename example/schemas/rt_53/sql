CREATE TABLE TBL(a) (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  b_id INTEGER
);

CREATE TABLE TBL(b) (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  someval INTEGER NOT NULL
);

INSERT INTO TBL(a) VALUES(NULL,NULL);
INSERT INTO TBL(a) VALUES(NULL,2);

INSERT INTO TBL(b) VALUES(NULL,111);
