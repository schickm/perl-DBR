CREATE TABLE TBL(abc) (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    a VARCHAR(255) NOT NULL,
    b VARCHAR(255) NOT NULL
);

CREATE INDEX abc_a ON TBL(abc) (a);
CREATE UNIQUE INDEX abc_b ON TBL(abc) (b);

CREATE TABLE TBL(def) (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    d VARCHAR(255) NOT NULL,
    e VARCHAR(255) NOT NULL,
    f VARCHAR(255) NOT NULL,
    g VARCHAR(255) NOT NULL,
    h VARCHAR(255) NOT NULL
);

CREATE UNIQUE INDEX def_de ON TBL(def) (d,e);
CREATE INDEX def_gf ON TBL(def) (g, f /*! (8) */);
CREATE INDEX def_gh ON TBL(def) (g, h);
CREATE INDEX def_gde ON TBL(def) (g, d, e);

CREATE TABLE TBL(dectest) (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    Q(dec) DECIMAL(5,2)
);
