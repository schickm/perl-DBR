CREATE TABLE abc (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    a VARCHAR(255) NOT NULL,
    b VARCHAR(255) NOT NULL
);

CREATE INDEX abc_a ON abc (a);
CREATE UNIQUE INDEX abc_b ON abc (b);

CREATE TABLE DEF (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    d VARCHAR(255) NOT NULL,
    e VARCHAR(255) NOT NULL,
    f VARCHAR(255) NOT NULL,
    g VARCHAR(255) NOT NULL,
    h VARCHAR(255) NOT NULL
);

CREATE UNIQUE INDEX def_de ON def (d,e);
CREATE INDEX def_gf ON def (g, f);
CREATE INDEX def_gh ON def (g, h);
CREATE INDEX def_gde ON def (g, d, e);

CREATE TABLE dectest (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    dec DECIMAL(5,2)
);
