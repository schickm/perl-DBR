CREATE TABLE cdc_badthing ( id INTEGER PRIMARY KEY AUTOINCREMENT );

CREATE TABLE tbl_normal ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE tbl_cdcbadcol ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_row_version INTEGER UNSIGNED NOT NULL );

CREATE TABLE badver ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_row_version INTEGER );
CREATE TABLE cdc_log_badver ( id INTEGER );

CREATE TABLE unknowncdc ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_blah INTEGER );
CREATE TABLE cdc_log_unknowncdc ( id INTEGER );

CREATE TABLE unknowncdc2 ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_unknowncdc2 ( id INTEGER, cdc_blah INTEGER );

CREATE TABLE badstart ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_badstart ( cdc_start_time SMALLINT );

CREATE TABLE badend ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_badend ( cdc_end_time SMALLINT );

CREATE TABLE badlogrowver1 ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_badlogrowver1 ( cdc_row_version SMALLINT );

CREATE TABLE badlogrowver2 ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_row_version TINYINT UNSIGNED NOT NULL );
CREATE TABLE cdc_log_badlogrowver2 ( id INTEGER PRIMARY KEY, cdc_row_version TINYINT UNSIGNED );

CREATE TABLE spurious1 ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_spurious1 ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo INTEGER );

CREATE TABLE spurious2 ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo TINYINT );
CREATE TABLE cdc_log_spurious2 ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo INTEGER );

CREATE TABLE missing_stime ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_missing_stime ( id INTEGER PRIMARY KEY AUTOINCREMENT );

CREATE TABLE missing_suser ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_missing_suser ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL );

CREATE TABLE missing_ver ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_row_version TINYINT UNSIGNED NOT NULL );
CREATE TABLE cdc_log_missing_ver ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED );

CREATE TABLE missing_etime1 ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_row_version TINYINT UNSIGNED NOT NULL );
CREATE TABLE cdc_log_missing_etime1 ( id INTEGER, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, cdc_row_version TINYINT UNSIGNED, PRIMARY KEY (id, cdc_row_version) );

CREATE TABLE missing_etime2 ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_missing_etime2 ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, cdc_end_user INTEGER UNSIGNED );

CREATE TABLE spurious_etime ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_spurious_etime ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, cdc_end_time INTEGER UNSIGNED NOT NULL );

CREATE TABLE missing_other ( id INTEGER PRIMARY KEY AUTOINCREMENT, bar INTEGER );
CREATE TABLE cdc_log_missing_other ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED );

CREATE TABLE good_c ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo VARCHAR(20) );
CREATE TABLE good_cu ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo VARCHAR(20), cdc_row_version SMALLINT UNSIGNED NOT NULL );
CREATE TABLE good_cd ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo VARCHAR(20) );
CREATE TABLE good_cud ( id INTEGER PRIMARY KEY AUTOINCREMENT, foo VARCHAR(20), cdc_row_version SMALLINT UNSIGNED NOT NULL );
CREATE TABLE cdc_log_good_c ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, foo VARCHAR(20) );
CREATE TABLE cdc_log_good_cu ( id INTEGER NOT NULL, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, foo VARCHAR(20), cdc_row_version SMALLINT UNSIGNED NOT NULL, cdc_end_time INTEGER UNSIGNED NOT NULL, PRIMARY KEY (id, cdc_row_version) );
CREATE TABLE cdc_log_good_cd ( id INTEGER PRIMARY KEY AUTOINCREMENT, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, foo VARCHAR(20), cdc_end_time INTEGER UNSIGNED NOT NULL, cdc_end_user INTEGER UNSIGNED );
CREATE TABLE cdc_log_good_cud ( id INTEGER NOT NULL, cdc_start_time INTEGER UNSIGNED NOT NULL, cdc_start_user INTEGER UNSIGNED, foo VARCHAR(20), cdc_end_time INTEGER UNSIGNED NOT NULL, cdc_end_user INTEGER UNSIGNED, cdc_row_version SMALLINT UNSIGNED NOT NULL, PRIMARY KEY (id, cdc_row_version) );


CREATE TABLE with_log_1 ( id INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE cdc_log_with_log_1 ( id INTEGER );

