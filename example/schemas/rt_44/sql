create table TBL(cart) (
 id integer primary key AUTOINCREMENT,
 name varchar(32),
 price int unsigned
);

insert into TBL(cart) (id,name,price) values (1,'apple' ,111);
insert into TBL(cart) (id,name,price) values (2,'orange',222);
insert into TBL(cart) (id,name,price) values (3,'fudge' ,null);
insert into TBL(cart) (id,name,price) values (4,'pear'  ,333);

-- devilishly priced
